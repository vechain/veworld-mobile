{
    "name": "VeWorld",
    "displayName": "VeWorld",
    "experiments": {
        "turboModules": true
    },
    "plugins": [
        "expo-font",
        "expo-secure-store",
        [
            "expo-local-authentication",
            {
                "faceIDPermission": "Allow $(PRODUCT_NAME) to use Face ID."
            }
        ],
        [
            "expo-build-properties",
            {
              "ios": {
                "newArchEnabled": true
              },
              "android": {
                "newArchEnabled": true
              }
            }
          ]
    ],
    "ios": {
        "scheme": "VeWorld"
    }
}
