{
  "name": "veworld",
  "version": "2.2.6",
  "private": true,
  "scripts": {
    "purge": "rm -rf node_modules && rm -rf android/app/build && cd ios && pod deintegrate && rm -rf Podfile.lock && cd ..",
    "purge:android": "rm -rf node_modules && rm -rf android/app/build",
    "install:all": "yarn install && npx pod-install",
    "install:android": "yarn install",
    "reinstall": "yarn purge && yarn install:all",
    "android": "react-native run-android",
    "android:emus": "react-native run-android --list-devices",
    "ios": "SKIP_SENTRY=true react-native run-ios --simulator 'iPhone 16 Pro'",
    "ios:old-device": "SKIP_SENTRY=true react-native run-ios --simulator 'iPhone SE (3rd generation)'",
    "start": "watchman watch-del './'; watchman watch-project './'; react-native start --reset-cache",
    "start:watchman": "watchman watch-del './'; watchman watch-project './'; react-native start",
    "start:reset": "react-native start --reset-cache",
    "start:i18n": "yarn i18n:types & react-native start & wait",
    "start:i18n:reset": "yarn i18n:types & react-native start --reset-cache & wait",
    "start:test": "REACT_APP_UI_LOG=false react-native start --reset-cache",
    "test": "jest",
    "test:specific": "jest --watch --verbose <filepath>",
    "test:troubleshoot": "jest --detectOpenHandles",
    "test:coverage:debug": "yarn test --forceExit --coverage",
    "test:coverage": "yarn test:coverage:debug --silent",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "yarn prettier --write src",
    "format:check": "yarn prettier --check src",
    "postinstall": "patch-package --exclude 'nothing'",
    "check-circular-dependencies": "npx madge --circular --warning --extensions js,jsx,ts,tsx .",
    "dapps-index:generate": "ts-node scripts/generate-dapps-index.ts",
    "i18n:types": "typesafe-i18n",
    "i18n:generate": "dotenv -e .env.local translo-cli",
    "i18n:generate-diff": "ts-node scripts/remove-changed-translation-keys.ts && yarn i18n:generate",
    "release:ci": "release-it --ci",
    "prepare": "husky",
    "e2e.ios.build.d": "SKIP_SENTRY=true detox build --configuration ios.sim.debug",
    "e2e.ios.build.r": "detox build --configuration ios.sim.release",
    "e2e.ios.test.d": "detox test --configuration ios.sim.debug_xcode_14.2 --parallel 4",
    "e2e.ios.test.d:trace": "detox test --configuration ios.sim.debug_xcode_14.2 --loglevel trace --record-logs all --parallel 4",
    "e2e.ios.test.d:sync": "detox test --configuration ios.sim.debug_xcode_14.2 detox test --debug-synchronization 500 --parallel 4",
    "e2e.ios.test.d:headless": "detox test --configuration ios.sim.debug detox test --headless --parallel 4",
    "e2e.ios.test.d:headless:screenshots": "detox test --configuration ios.sim.debug detox test --headless --take-screenshots failing --parallel 4",
    "e2e.ios.test.r": "detox test --configuration ios.sim.release",
    "e2e.ios.r": "yarn e2e.ios.build.r && yarn e2e.ios.test.r",
    "e2e.ios.d": "yarn e2e.ios.build.d && yarn e2e.ios.test.d",
    "e2e.android.build.d": "detox build --configuration android.emu.debug.32",
    "e2e.android.test.d": "detox test --configuration android.emu.debug.32",
    "e2e.android.test.d:sync": "detox test --configuration android.emu.debug.33",
    "e2e.android.test.d:trace": "detox test --configuration android.emu.debug.31 --loglevel trace --record-logs all",
    "e2e.android.d": "yarn e2e.android.build.d && yarn e2e.android.test.d",
    "e2e.android.build.r": "detox build --configuration android.emu.release.33",
    "e2e.android.test.r": "detox test --configuration android.emu.release.33",
    "e2e.android.r": "yarn e2e.android.build.r && yarn e2e.android.test.r",
    "lint-staged": "lint-staged",
    "translo-cli": "translo-cli",
    "abi:build": "cd ./cli-tools/json-abi-to-ts && yarn build",
    "abi:install": "cd ./cli-tools/json-abi-to-ts && yarn",
    "abi:generate-json-abi-file": "$(yarn bin json-abi-to-ts) generate-object --input-directory ./abi-resources/abis --out-file ./abi-resources/generated-abi.json --output json",
    "abi:generate-ts-abi-file": "$(yarn bin json-abi-to-ts) generate-object --input-directory ./abi-resources/abis --out-file ./src/Generated/abi.ts --output ts",
    "abi:generate-business-events-directory": "$(yarn bin json-abi-to-ts) validate-update-business-events --input-directory ./abi-resources/business_events --out-directory ./abi-resources/business_events --generated-event-input-file ./abi-resources/generated-abi.json",
    "abi:generate-ts-business-events-file": "$(yarn bin json-abi-to-ts) generate-object --input-directory ./abi-resources/business_events --out-file ./src/Generated/businessEvents.ts --output ts",
    "abi:generate": "yarn abi:generate-json-abi-file && yarn abi:generate-ts-abi-file && yarn abi:generate-business-events-directory && yarn abi:generate-ts-business-events-file"
  },
