    "e2e.ios.test.r": "detox test --configuration ios.sim.release",
    "e2e.android.build.d": "detox build --configuration android.emu.debug",
    "e2e.android.build.r": "detox build --configuration android.emu.release",
    "e2e.android.test.d": "detox test --configuration android.emu.debug",
    "e2e.android.test.r": "detox test --configuration android.emu.release",
    "typesafe-i18n": "typesafe-i18n"
  },
  "dependencies": {
    "@craftzdog/react-native-buffer": "^6.0.5",
