{
    "name": "veWorldMobile",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "purge": "rm -rf node_modules && rm -rf yarn.lock && rm -rf android/app/build && cd ios && pod deintegrate && rm -rf Podfile.lock && cd ..",
        "install:all": "yarn install && npx pod-install && yarn husky install && patch-package",
        "doctor": "react-native doctor",
        "android": "react-native run-android",
        "ios": "react-native run-ios --simulator 'iPhone 14 Pro'",
        "start": "react-native start",
        "test": "jest",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "format": "yarn prettier --write src",
        "format:check": "yarn prettier --check src",
        "e2e.ios.build.d": "detox build --configuration ios.sim.debug",
        "e2e.ios.build.r": "detox build --configuration ios.sim.release",
        "e2e.ios.test.d": "detox test --configuration ios.sim.debug",
        "e2e.ios.test.r": "detox test --configuration ios.sim.release",
        "e2e.android.build.d": "detox build --configuration android.emu.debug",
        "e2e.android.build.r": "detox build --configuration android.emu.release",
        "e2e.android.test.d": "detox test --configuration android.emu.debug",
        "e2e.android.test.r": "detox test --configuration android.emu.release",
        "typesafe-i18n": "typesafe-i18n"
    },
    "dependencies": {
        "@craftzdog/react-native-buffer": "^6.0.5",
        "@gorhom/bottom-sheet": "^4.4.5",
        "@klarna/react-native-vector-drawable": "^0.4.0",
        "@react-native-community/blur": "^4.3.0",
        "@react-navigation/bottom-tabs": "^6.5.2",
        "@react-navigation/material-top-tabs": "^6.5.3",
        "@react-navigation/native": "^6.1.1",
        "@react-navigation/native-stack": "^6.9.7",
        "@shopify/flash-list": "^1.4.1",
        "@vechain/connex-driver": "^2.0.12",
        "@vechain/connex-framework": "^2.0.12",
        "@vechain/picasso": "^2.1.1",
        "babel-plugin-module-resolver": "^4.1.0",
        "eth-rpc-errors": "^4.0.3",
        "expo": "^47.0.9",
        "expo-clipboard": "~4.0.1",
        "expo-font": "~11.0.1",
        "expo-haptics": "~12.0.1",
        "expo-local-authentication": "~13.0.2",
        "expo-localization": "~14.0.0",
        "expo-secure-store": "~12.0.0",
        "https-browserify": "^1.0.0",
        "immer": "^9.0.18",
        "jsdoc": "^4.0.0",
        "lodash.isequal": "^4.5.0",
        "patch-package": "^6.5.1",
        "react": "18.1.0",
        "react-native": "0.70.6",
        "react-native-bootsplash": "^4.4.2",
        "react-native-bouncy-checkbox": "^3.0.6",
        "react-native-draggable-flatlist": "4.0.1",
        "react-native-drop-shadow": "^0.0.6",
        "react-native-flipper": "0.163.0",
        "react-native-exception-handler": "^2.10.10",
        "react-native-gesture-handler": "^2.9.0",
        "react-native-get-random-values": "^1.8.0",
        "react-native-pager-view": "^6.1.2",
        "react-native-quick-crypto": "^0.5.0",
        "react-native-reanimated": "^2.14.4",
        "react-native-reanimated-carousel": "^3.3.0",
        "react-native-responsive-linechart": "^5.7.1",
        "react-native-restart": "^0.0.27",
        "react-native-safe-area-context": "^4.4.1",
        "react-native-screens": "^3.18.2",
        "react-native-svg": "13.4.0",
        "react-native-tab-view": "^3.3.4",
        "react-native-vector-icons": "^9.2.0",
        "realm": "11.3.1",
        "realm-flipper-plugin-device": "^1.1.0",
        "stream-browserify": "^3.0.0",
        "stream-http": "^3.2.0",
        "thor-devkit": "https://github.com/vechainfoundation/thor-devkit.js",
        "url": "^0.11.0"
    },
    "devDependencies": {
        "@babel/core": "^7.12.9",
        "@babel/runtime": "^7.12.5",
        "@commitlint/cli": "^17.3.0",
        "@commitlint/config-conventional": "^17.3.0",
        "@cucumber/cucumber": "^8.10.0",
        "@formatjs/intl-getcanonicallocales": "^2.0.5",
        "@formatjs/intl-locale": "^3.0.11",
        "@formatjs/intl-pluralrules": "^5.1.8",
        "@react-native-community/eslint-config": "^2.0.0",
        "@tsconfig/react-native": "^2.0.2",
        "@types/cucumber": "^7.0.0",
        "@types/detox": "^18.1.0",
        "@types/jest": "^26.0.23",
        "@types/react": "^18.0.21",
        "@types/react-native": "^0.70.6",
        "@types/react-native-vector-icons": "^6.4.12",
        "@types/react-test-renderer": "^18.0.0",
        "@typescript-eslint/eslint-plugin": "^5.37.0",
        "@typescript-eslint/parser": "^5.37.0",
        "@vechain/connex-types": "^2.0.12",
        "babel-jest": "^26.6.3",
        "detox": "^20.1.0",
        "eslint": "^7.32.0",
        "eslint-plugin-i18next": "^6.0.0-2",
        "husky": "^8.0.2",
        "jest": "^29",
        "lint-staged": "^13.1.0",
        "metro-react-native-babel-preset": "0.72.3",
        "react-test-renderer": "18.1.0",
        "ts-node": "^10.9.1",
        "typesafe-i18n": "^5.18.0",
        "typescript": "^4.8.3"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "yarn prettier --check src",
            "eslint . --ext .js,.jsx,.ts,.tsx",
            "git add"
        ],
        "*.{html,css,less,ejs}": [
            "prettier --write",
            "git add"
        ]
    },
    "jest": {
        "preset": "react-native",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    }
}