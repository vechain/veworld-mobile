{
    "name": "veworld",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "purge": "rm -rf node_modules && rm -rf yarn.lock && rm -rf android/app/build && cd ios && pod deintegrate && rm -rf Podfile.lock && cd ..",
        "install:all": "yarn install && npx pod-install",
        "postinstall": "patch-package",
        "pods": "cd ios && pod install && cd ..",
        "doctor": "react-native doctor",
        "android": "react-native run-android",
        "ios": "react-native run-ios --simulator 'iPhone 14 Pro'",
        "ios:old-device": "react-native run-ios --simulator 'iPhone SE (3rd generation)'",
        "start": "react-native start",
        "start:reset": "react-native start --reset-cache",
        "start:i18n": "yarn i18n & react-native start & wait",
        "test": "jest",
        "test:coverage:debug": "yarn test --forceExit --coverage",
        "test:coverage": "yarn test:coverage:debug --silent",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "format": "yarn prettier --write src",
        "format:check": "yarn prettier --check src",
        "e2e.ios.build.d": "detox build --configuration ios.sim.debug",
        "e2e.ios.build.r": "detox build --configuration ios.sim.release",
        "e2e.ios.test.d": "detox test --configuration ios.sim.debug",
        "e2e.ios.test.r": "detox test --configuration ios.sim.release",
        "e2e.android.build.d": "detox build --configuration android.emu.debug.31",
        "e2e.android.build.r": "detox build --configuration android.emu.release.31",
        "e2e.android.test.d": "detox test --configuration android.emu.debug.31",
        "e2e.android.test.r": "detox test --configuration android.emu.release.31",
        "i18n": "typesafe-i18n",
        "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios' --assets-dest='./iOS'",
        "prepare": "husky install"
    },
    "dependencies": {
        "@craftzdog/react-native-buffer": "^6.0.5",
        "@gorhom/bottom-sheet": "^4.4.5",
        "@klarna/react-native-vector-drawable": "^0.4.0",
        "@react-native-community/blur": "^4.3.0",
        "@react-navigation/bottom-tabs": "^6.5.2",
        "@react-navigation/material-top-tabs": "^6.6.2",
        "@react-navigation/native": "^6.1.1",
        "@react-navigation/native-stack": "^6.9.7",
        "@reduxjs/toolkit": "^1.9.3",
        "@shopify/flash-list": "^1.4.1",
        "@vechain/connex-driver": "^2.0.12",
        "@vechain/connex-framework": "^2.0.12",
        "@vechain/picasso": "^2.1.1",
        "axios": "^0.27.2",
        "babel-plugin-module-resolver": "^4.1.0",
        "eth-rpc-errors": "^4.0.3",
        "expo": "^47.0.9",
        "expo-barcode-scanner": "~12.1.0",
        "expo-camera": "~13.1.0",
        "expo-clipboard": "~4.0.1",
        "expo-font": "~11.0.1",
        "expo-haptics": "~12.0.1",
        "expo-local-authentication": "~13.0.2",
        "expo-localization": "~14.0.0",
        "expo-secure-store": "~12.0.0",
        "https-browserify": "^1.0.0",
        "immer": "^9.0.18",
        "jest": "^29.3.1",
        "jest-expo": "^48.0.2",
        "jsdoc": "^4.0.0",
        "json-stringify-safe": "^5.0.1",
        "lodash": "^4.17.21",
        "lottie-react-native": "^6.0.0-rc.3",
        "patch-package": "^6.5.1",
        "promise.allsettled": "^1.0.6",
        "react": "18.1.0",
        "react-native": "0.70.6",
        "react-native-bootsplash": "^4.4.2",
        "react-native-bouncy-checkbox": "^3.0.6",
        "react-native-draggable-flatlist": "4.0.1",
        "react-native-drop-shadow": "^0.0.6",
        "react-native-exception-handler": "^2.10.10",
        "react-native-fast-image": "^8.6.3",
        "react-native-gesture-handler": "^2.9.0",
        "react-native-haptic-feedback": "^1.14.0",
        "react-native-mmkv": "2.5.1",
        "react-native-pager-view": "^6.2.0",
        "react-native-quick-crypto": "^0.5.0",
        "react-native-reanimated": "^2.14.4",
        "react-native-reanimated-carousel": "^3.3.0",
        "react-native-restart": "^0.0.27",
        "react-native-safe-area-context": "^4.4.1",
        "react-native-screens": "^3.18.2",
        "react-native-svg": "13.4.0",
        "react-native-swipeable-item": "^2.0.9",
